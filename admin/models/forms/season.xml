<?xml version="1.0" encoding="utf-8" ?>
<form>
	<fieldset>
		<field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL" readonly="true" class="readonly" description="JGLOBAL_FIELD_ID_DESC" />
		<field name="name" type="text" class="inputbox" size="60" label="COM_TTLIVESCORE_FIELD_NAME_LABEL" description="COM_TTLIVESCORE_FIELD_NAME_DESC" required="true" />
		<field name="published" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1" default="1">
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>
		<field name="country" type="Countries" class="inputbox" label="COM_TTLIVESCORE_FIELD_COUNTRY_LABEL" description="COM_TTLIVESCORE_FIELD_COUNTRY_DESC" size="1" required="true" />
		<field name="startdate" type="calendar" label="COM_TTLIVESCORE_FIELD_STARTDATE_LABEL" description="COM_TTLIVESCORE_FIELD_STARTDATE_DESC" class="inputbox" size="22" filter="user-utc" />
		<field name="enddate" type="calendar" label="COM_TTLIVESCORE_FIELD_ENDDATE_LABEL" description="COM_TTLIVESCORE_FIELD_ENDDATE_DESC" class="inputbox" size="22" filter="user-utc" />
	</fieldset>
</form>
